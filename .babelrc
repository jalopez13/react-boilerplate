{
  "presets": [
    ["@babel/preset-env", {
      "targets": [
        "last 2 versions", 
        "not dead",
        "not < 2%",
        "not ie 11"
      ],
      "corejs": "3.4.1",
      "useBuiltIns": "entry"
    }],
    "@babel/preset-react" 
  ],
  "plugins": [
    "babel-plugin-transform-class-properties",
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-syntax-dynamic-import"
  ]

}
